<h3>Articles</h3>
<div class="container">
  <% @articles.each do |article|%>
    <div class="card">
      <%= link_to "#{article.title}", article_path(article.id) %><span><%= link_to "Delete", article_path(article), method: :delete %></span>
    </div>
  <% end %>
  <%= link_to "Add an Article", new_article_path %>
</div>
